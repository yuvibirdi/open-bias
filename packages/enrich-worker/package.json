{
  "name": "@open-bias/enrich-worker",
  "main": "./src/index.ts",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.14.0",
    "@open-bias/db": "workspace:*",
    "@open-bias/common": "workspace:*",
    "natural": "^7.0.0",
    "openai": "^5.8.2"
  },
  "scripts": {
